{% extends 'base.html.twig' %}

{% block title %}Experience{% endblock %}

{% block body %}
    <h1>Experience</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ experience.id }}</td>
            </tr>
            <tr>
                <th>Ids</th>
                <td>{{ experience.ids }}</td>
            </tr>
            <tr>
                <th>DateD</th>
                <td>{{ experience.dateD ? experience.dateD|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateF</th>
                <td>{{ experience.dateF ? experience.dateF|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>NomEtablissement</th>
                <td>{{ experience.nomEtablissement }}</td>
            </tr>
            <tr>
                <th>VilleEtablissement</th>
                <td>{{ experience.villeEtablissement }}</td>
            </tr>
            <tr>
                <th>NomPoste</th>
                <td>{{ experience.nomPoste }}</td>
            </tr>
            <tr>
                <th>Mission</th>
                <td>{{ experience.mission }}</td>
            </tr>
            <tr>
                <th>Responsable</th>
                <td>{{ experience.responsable ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_experience_index') }}">back to list</a>

    <a href="{{ path('app_experience_edit', {'id': experience.id}) }}">edit</a>

    {{ include('experience/_delete_form.html.twig') }}
{% endblock %}
